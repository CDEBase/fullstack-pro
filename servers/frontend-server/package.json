{
	"name": "sample-frontend-server",
	"version": "0.0.5",
	"description": "Sample Client server",
	"private": true,
	"main": "index.js",
	"scripts": {
		"docker:build": "npm run build && docker build . -t $npm_package_name:$npm_package_version",
		"docker:run": "docker run -it $npm_package_name:$npm_package_version",
		"prebuild": "npm run build:clean",
		"build:clean": "rimraf dist",
		"build": "cross-env NODE_ENV=production spin build",
		"start": "node --harmony dist",
		"start:staging": "cross-env NODE_ENV=staging ENV_FILE=../../config/staging/staging.env node --harmony dist",
		"watch": "spin watch",
		"watch:debug": "npm run watch -- -v",
		"watch:staging": "cross-env ENV_FILE=../../config/staging/staging.env spin watch",
		"jest": "./node_modules/.bin/jest",
		"test": "jest",
		"test:debug": "npm test -- --runInBand",
		"test:watch": "npm test -- --watch"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/cdmbase/fullstack-pro.git"
	},
	"author": "CDMBase LLC",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/cdmbase/fullstack-pro/issues"
	},
	"homepage": "https://github.com/cdmbase/fullstack-pro#readme",
	"devDependencies": {
		"cross-env": "^5.0.5",
		"jest": "^21.1.0",
		"rimraf": "^2.6.1",
		"spinjs": "^0.4.26",
		"webpack": "^3.7.1"
	},
	"dependencies": {
		"@cdm-logger/client": "^3.1.1",
