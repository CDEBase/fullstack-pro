{
	"name": "sample-stack-frontend-server",
	"version": "0.0.10",
	"description": "Sample Client server",
	"private": true,
	"main": "index.js",
	"scripts": {
		"docker:build": "npm run build && docker build . -t $npm_package_name:$npm_package_version",
		"docker:run": "docker run  --env-file ../../config/test/docker-test.env  -p 3010:3010  -it $npm_package_name:$npm_package_version",
		"prebuild": "npm run build:clean",
		"build:clean": "rimraf dist",
		"build": "cross-env NODE_ENV=production spin build",
		"start": "node --harmony dist",
		"start:test": "cross-env NODE_ENV=test ENV_FILE=../../config/test/test.env node --harmony dist",
		"watch": "cross-env SSR=true NODE_ENV=development ENV_FILE=../../config/development/dev.env spin watch",
		"watch:debug": "npm run watch -- -v",
		"watch:test": "cross-env ENV_FILE=../../config/test/test.env spin watch",
		"jest": "./node_modules/.bin/jest",
		"test": "jest",
		"test:debug": "npm test -- --runInBand",
		"test:watch": "npm test -- --watch"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/cdmbase/fullstack-pro.git"
	},
	"author": "CDMBase LLC",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/cdmbase/fullstack-pro/issues"
	},
	"homepage": "https://github.com/cdmbase/fullstack-pro#readme",
	"devDependencies": {
