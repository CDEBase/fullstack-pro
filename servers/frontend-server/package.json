    "fela-plugin-unit": "^10.2.4",
    "fela-plugin-validator": "^10.2.4",
    "fela-preset-web": "^10.2.4",
    "graphql": "^14.2.1",
    "graphql-tag": "^2.10.1",
    "history": "^4.9.0",
    "immutability-helper": "^3.0.0",
    "inversify": "^5.0.1",
