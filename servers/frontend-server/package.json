		"@sample-stack/utils": "*",
		"apollo-client": "^1.9.3",
		"apollo-link": "^0.7.0",
		"apollo-logger": "^0.1.0",
		"browser-bunyan": "^0.4.0",
		"persistgraphql": "^0.3.10",
		"bunyan": "^1.8.10",
