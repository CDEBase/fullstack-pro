{
	"name": "sample-stack-backend-server",
	"version": "0.0.10",
	"description": "Starter kit for apollo server using webpack and typescript",
	"main": "dist/index.js",
	"private": true,
	"scripts": {
		"prepublish": "npm run build:clean",
		"build:clean": "rimraf dist",
		"build": "cross-env NODE_ENV=production spin build",
		"start": "node --harmony dist",
		"start:staging": "cross-env ENV_FILE=../../config/staging/staging.env node --harmony dist",
		"watch": "spin watch",
		"watch:staging": "cross-env NODE_ENV=staging ENV_FILE=../../config/staging/staging.env spin watch",
		"test": "jest",
		"test:watch": "npm test -- --watch",
		"test:notify": "npm run test:watch -- --notify",
		"docker:build": "npm run build && docker build . -t $npm_package_name:$npm_package_version",
		"docker:run": "docker run -it $npm_package_name:$npm_package_version",
		"preupver": "npm test",
		"upver": "standard-version",
		"knex": "ts-node --no-project ./node_modules/.bin/knex",
		"db:migrate": "npm run knex -- migrate:latest --cwd . --knexfile ./knexfile.ts",
		"db:migrate:rollback": "npm run knex -- migrate:rollback  --cwd . --knexfile ./knexfile.ts",
		"db:seed": "npm run knex -- seed:run  --cwd . --knexfile ./knexfile.ts",
		"stagedb:migrate": "cross-env ENV_FILE=../../config/staging/staging.env NODE_ENV=staging npm run db:migrate",
		"stagedb:migrate:rollback":  "cross-env ENV_FILE=../../config/staging/staging.env NODE_ENV=staging npm run db:migrate:rollback",
		"stagedb:seed":  "cross-env ENV_FILE=../../config/staging/staging.env NODE_ENV=staging npm run db:seed",
		"proddb:migrate": "NODE_ENV=staging npm run db:migrate",
		"proddb:migrate:rollback":  "NODE_ENV=staging npm run db:migrate:rollback",
		"proddb:seed":  "NODE_ENV=staging npm run db:seed"
	},
	"keywords": [
		"apollo",
		"backend",
		"apollo-server",
		"typescript",
		"webpack",
		"graphql",
		"graphiql",
		"express"
	],
	"repository": {
		"type": "git",
		"url": "git+https://github.com/cdmbase/fullstack-pro.git"
	},
	"author": "CDMBase LLC",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/cdmbase/fullstack-pro/issues"
	},
	"homepage": "https://github.com/cdmbase/fullstack-pro#readme",
	"dependencies": {
		"@sample-stack/graphql-schema": "*",
		"@sample-stack/server-core": "*",
		"@sample-stack/core": "*",
		"@sample-stack/store": "*",
		"@sample-stack/graphql-gql": "*",
		"@sample-stack/utils": "*",
		"@cdm-logger/server": "^3.0.2",
		"dotenv": "^5.0.0",
		"apollo-logger": "^0.1.0",
		"app-root-path": "^2.0.1",
		"reflect-metadata": "^0.1.10",
		"lodash": "^4.17.4",
		"knex": "^0.14.2",
		"subscriptions-transport-ws": "^0.9.5",
		"isomorphic-fetch": "^2.2.1",
		"inversify": "^4.10.0",
		"morgan": "^1.8.1",
		"nats": "^0.7.24",
		"persistgraphql": "^0.3.11",
		"nats-hemera": "^3.1.7",
		"graphql": "^0.11.7",
		"graphql-subscriptions": "^0.5.6",
		"graphql-tools": "2.19.0",
		"graphql-nats-subscriptions": "^1.1.0",
		"sqlite3": "^3.1.8",
		"ws": "^3.3.3",
		"graphql-server-express": "^1.3.2",
		"express": "^4.16.2",
		"cors": "^2.8.3",
		"body-parser": "^1.18.2",
		"apollo-link": "^1.0.7"
	},
	"peerDependencies": {
		"subscriptions-transport-ws": "*",
		"body-parser": "*",
		"cors": "*",
		"knex": "*",
		"express": "*",
		"concurrently": "*",
		"helmet": "*",
		"morgan": "*"
	},
	"devDependencies": {
		"cross-env": "^5.1.3",
		"spinjs": "0.4.18",
		"ts-node": "^4.1.0",
		"typescript": "2.7.1",
		"jest": "^22.1.0",
		"rimraf": "^2.6.1",
		"webpack": "^3.10.0"
	},
	"typings": "dist/main.d.ts",
	"typescript": {
		"definition": "dist/main.d.ts"
	}
}