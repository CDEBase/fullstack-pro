{
    "expo": {
      "name": "mobile-layout",
      "description": "A React Native mobile app based on Apollo Universal Starter Kit",
      "version": "1.0.0",
      "slug": "apollo-universal-starter-kit",
      "ios": {
        "bundleIdentifier": "com.cdebase.apollokit"
      },
      "android": {
        "package": "com.cdebase.apollokit"
      },
      "packagerOpts": {
        "config": "metro.config.js",
        "sourceExts": [
          "native.tsx",
          "native.jsx",
          "native.ts",
          "native.js",
          "jsx",
          "js",
          "json",
          "ts",
          "tsx",
          "graphql",
          "gql"
        ]
      }
    }
  }