{
  "name": "@sample/schema",
  "version": "0.0.1",
  "description": "Sample Schema Module",
  "main": "lib/index.js",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "start": "webpack -p --config --progress --watch",
    "test": "npm run testonly",
    "test-ci": "npm lint && npm coverage",
    "testonly": "jest",
    "prepublish": "npm run build",
    "build": "npm run build:clean && npm run build:lib",
    "build:clean": "rimraf lib",
    "build:lib": "webpack",
    "build:lib:watch": "npm run build:lib -- --watch"
  },
  "jest": {
    "testEnvironment": "node",
    "testRegex": "(/.*.(test|spec)).(js|jsx)$",
    "roots": [
      "src"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/js-accounts/accounts/tree/master/packages/server"
  },
  "author": "CDMBase LLC",
  "license": "MIT",
  "dependencies": {
    "cdm-logger": "^0.3.2",
    "graphql-tools": "0.11.0",
    "lodash": "^4.17.4"
  },
  "devDependencies": {
    "@types/node": "^7.0.12",
    "@types/jest": "^19.2.2",
    "graphql-subscriptions": "^0.3.1",
    "eslint": "^3.19.0",
    "eslint-plugin-graphql": "^0.8.0",
    "graphql": "^0.9.2",
    "rimraf": "^2.6.1",
    "webpack": "^2.3.3"
  },
  "peerDependencies": {
    "graphql": "^0.9.0 || ^0.10.0"
  },
  "optionalDependencies": {
    "@types/graphql": "^0.9.2"
  },
  "typings": "lib/index.d.ts",
  "typescript": {
    "definition": "lib/index.d.ts"
  }
}