{
  "name": "@sample/graphql",
  "version": "0.0.1",
  "description": "GraphQL Client side queries and mutations for sample",
  "main": "lib/index.js",
  "scripts": {
    "typify": "gql-gen --url http://localhost:3000/graphql --template flow --out ./flow/decl/schema.js ./src/**/*.graphql",
    "jest": "./node_modules/.bin/jest",
    "test": "jest",
    "test:debug": "npm test -- --runInBand",
    "test:watch": "npm test -- --watch",
    "prepublish": "npm run build",
    "watch": "npm run build:lib:watch",
    "build": "npm run build:clean && npm run build:lib",
    "build:clean": "rimraf lib",
    "build:lib": "webpack",
    "build:lib:watch": "npm run build:lib -- --watch"
  },
  "publishConfig": {
    "access": "public"
  },
  "jest": {
    "testEnvironment": "node",
    "roots": [
      "src"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "\\.(ts|tsx)$": "<rootDir>/../../node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "/__tests__/.*\\.(ts|tsx|js)$"
  },
  "author": "CDMBase LLC",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cdmbase/fullstack-pro/issues"
  },
  "homepage": "https://github.com/cdmbase/fullstack-pro#readme",
  "devDependencies": {
    "@types/jest": "^19.2.2",
    "apollo-codegen": "^0.10.13",
    "eslint": "^3.19.0",
    "eslint-plugin-graphql": "^0.8.0",
    "graphql-code-generator": "^0.5.2",
    "jest": "^19.0.2",
    "raw-loader": "^0.5.1",
    "nock": "^9.0.11",
    "rimraf": "^2.6.1",
    "webpack": "^2.3.3",
    "cdm-webpack-node-externals": "^1.5.6"
  },
  "dependencies": {
    "graphql-tag": "^2.0.0"
  },
  "peerDependencies": {
    "graphql": "^0.9.0"
  },
  "typings": "lib/index.d.ts",
  "typescript": {
    "definition": "lib/index.d.ts"
  }
}